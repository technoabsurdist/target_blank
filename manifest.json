{
  "manifest_version": 3,
  "name": "Open Links in New Tabs",
  "version": "1.0",
  "description": "Make all links open in new tabs",
  "permissions": [
	  "activeTab",
	  "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}

