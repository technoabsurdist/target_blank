{
  "manifest_version": 5,
  "name": "Open Links in New Tabs",
  "version": "1.0",
  "description": "Make all links open in new tabs",
  "permissions": [
	  "activeTab",
	  "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
	    "128": "images/icon128.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  }
}

